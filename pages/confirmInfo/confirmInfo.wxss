@import "../common/lib/weui.wxss";
page{
    height: 100%;
}
.container{
  display: flex;
  background-color: #f5f5f9;
  flex-direction: column;
  width: 100%;
  height: 100%;
}
/* header{
  width:100%;height:100rpx;
  text-align: center; overflow: hidden;
} */
.confirmCenter{
  width:100%;
}
#content{
  background:#f1f0f6;
  flex:1;
  padding: 0;
  background: linear-gradient(left, #88a9ff, #7da7ff,#63a1ff);
  background: -webkit-linear-gradient(left, #88a9ff, #7da7ff,#63a1ff);
  overflow:scroll;
  height:2340rpx;
}
.footer{
  width:100%;
  height:100rpx;
}
.agree_box,.insurance_date,.dogInfo_box,.insurance_box,.disease_box,.header-box{
  width:100%;
}

.header-box{
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #ddd;
  height: 150rpx;
  flex: 1;
  margin-bottom: 20rpx;
  
}
.header-box-left{
  height: 150rpx;
  margin-top: 35rpx;
}
.header-box-left .title{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 40rpx;
}
.header-box-left .sm-title{
  height: 30rpx;
  line-height: 50rpx;
  color:#aaa;
  font-size: 26rpx;
}
.header-box-right{
  line-height: 150rpx;
  align-content: center;
}
.header-box-right image{
  width:32rpx;
  height:32rpx;
}
.header-box-right .inRule,.header-box-right .iconfont{
  /* align-items: center; */
  color:#f8e71c;font-size: 28rpx;
}
.disease_box .title{
  font-size: 38rpx;margin:20rpx 0;
}
.disease_box #content{
  display: flex;
  flex-flow: row wrap;
  font-size: 30rpx;margin:20rpx 0;
  padding:20rpx 30rpx 10rpx;
}
.disease_box #content text{
  width:50%;
}
.insurance_box .table{
  height:100rpx;
  border:1px solid;
  display: flex;
  margin: 0rpx 30rpx 16rpx;
}
.insurance_box .table-tr{
  flex:1;
  text-align: center;
  border-right:1px solid;
}
.insurance_box .table-tr:last-child{
  border:0;
}
.insurance_box .table-tr .tab-title{
  font-size: 26rpx;
}
.insurance_box .table-tr .tab-content{
  border:0;
}
.dogInfo_box .title{
  font-weight: bolder;
  font-size: 34rpx;
}
.dogInfo_box .title text{
  vertical-align: middle;
}
.dogInfo_box .title  .notice{
  color:#ff0000;
  font-weight: normal;
  font-size: 26rpx;
  margin-left: 30rpx;
}
.dogInfo_box .dogInfo{
  padding:30rpx;
}
.dogInfo_box .dogInfo-item{
  display: flex;  
  flex-direction: row;
  
}
.dogInfo_box .dogInfo-item-text{
    flex-grow: 1;
    
}
.insurance_date .title{
   font-weight: bolder;
   font-size: 34rpx;
}
.insurance_date #content{
   padding:30rpx;
} 
.agree_box{
  padding: 30rpx 0;
}
.agree_box checkbox-group{
  display: flex;
  align-items: center;
}
.agree_box .checkbox{
  display: flex;
  align-items: center;
}
.agree_box .checkbox text{
  vertical-align: middle;
  font-size: 28rpx;
  color:#fff;
  margin-left: 10rpx;
}
.agree_box .checkbox checkbox{
  zoom:50%;
  margin-top: -6rpx;
}
.agree_box .inRule{
  /* align-items: center; */
  color:#f8e71c;font-size: 28rpx;
}

.dogInfo-box{
  margin:30rpx 48rpx;
  box-shadow: 6rpx 10rpx 20rpx  #3d92ff;
  background: #fff;overflow: hidden;border-radius:16rpx ;
  
}
.dog{
  position: absolute;
  bottom: 0;
  width: 100%;
  
}
.dogInfo{
  /* border: 1px solid; */
  overflow: hidden;
  border-radius: 16rpx;
  font-size: 32rpx;
  padding-bottom: 30rpx;
}
.pedigree{
  background: rgb(234, 243, 255);
  color:rgb(49, 142, 255);
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 30rpx;overflow: hidden;
  text-align: center;
  font-weight: bold;font-size: 28rpx;
}
.dogInfoDetail{
  padding:0 30rpx;
}

.dogInfoDetail .dogInfo_key{
  color:#318eff;font-size: 26rpx;
}
.dogInfo-item{
  display: flex;  
  flex-direction: row;
}
.dogInfoDetail>view{
  height: 78rpx;
  line-height: 78rpx;
  border-bottom:1px dashed #d7eaff;
}
.confirmIcon{
  width:40rpx;
  height:40rpx;
  vertical-align:middle;
  margin-right: 8rpx;
}

.dogInfo-item-text{
    flex-grow: 1;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.agree_box{
  position: absolute;
  top:50%;left:50%;
  transform: translate(-50%,-50%);

}
.confirmUnSelect{
  width:26rpx;height:26rpx;
}
.agree_box>view{
  display: flex;
  align-items: center;
  justify-content: center;
}

.bottomBtn{
  display: flex;
  align-self: flex-end;
  /* flex: 1; */
}
.bottomBtn view{
  border-radius: none;
  margin: 0;padding:0;
  position: none;
  box-sizing: border-box;
  height:100rpx;
  overflow: hidden;
  flex-grow: 1;
  background: rgb(240, 240, 240);
}
.bottomBtn view:nth-child(2){
  flex-grow: 3;
  background: rgb(255, 164, 25);
}
.bottomBtn view button{
  background: rgb(240, 240, 240);
  line-height: 100rpx;
  height:100rpx;
  color: rgb(121, 121, 121);
}
.bottomBtn view:nth-child(2) button{
  background: rgb(255, 164, 25);color:#fff;
  
}
.bottomBtn button{
  margin-top: 0;
  margin-bottom: 0;
  height:100rpx;
  
}
.bottomBtn button::after{
  border-radius: none;
  border:none;
}

.iconfont.icon-zuo{
  font-size: 40rpx;line-height:64rpx;
}
.prev_box{
  position:fixed;top:48rpx;height:64rpx;line-height:64rpx;color:#fff;margin:0 20rpx;font-size:40rpx;
}
#topLoad1{
  background:#fff;

}