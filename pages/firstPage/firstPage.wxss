/* pages/firstPage/firstPage.wxss */
.container,page{
  min-width:100%;
  height:100%;
  background:#E8EEFF;
}
.dogInfo-box{
  margin:30rpx 24rpx;
  box-shadow: 6rpx 10rpx 6rpx  #c6d7ef;
  overflow: hidden; 
}
.dogInfo{
  /* border: 1px solid; */
  overflow: hidden;
  border-radius: 16rpx;
  font-size: 32rpx;
  padding-bottom: 30rpx;
}
.pedigree{
  background: -webkit-linear-gradient(left bottom,rgb(144, 202, 255),rgb(216, 237, 255));
  color:#fff;
  height: 90rpx;
  line-height: 90rpx;
  padding-left: 30rpx;overflow: hidden;
  font-size: 26rpx;
}
.dogInfoDetail{
  padding:0 30rpx;
}

.dogInfoDetail .dogInfo_key{
  color:#318eff;font-size: 26rpx;
}
.dogInfo-item{
  display: flex;  
  flex-direction: row;
}
.dogInfoDetail>view{
  height: 78rpx;
  line-height: 78rpx;
  border-bottom:1px solid #d7eaff;
}

.dogInfo-item-text{
    flex-grow: 1;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}