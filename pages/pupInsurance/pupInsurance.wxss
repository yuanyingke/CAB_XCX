@import '../common/lib/weui.wxss';
.imgBox{
  width:100%;
  height: 300rpx;
}
.imgBox image{
  width: 100%;
  height: 100%;
}
/* pages/pupInsurance/pupInsurance.wxss */
.insurance_box .table{
  height:100rpx;
  border:1px solid;
  display: flex;
  /* margin: 0rpx 30rpx 16rpx; */
}
.insurance_box .table-tr{
  flex:1;
  text-align: center;
  border-right:1px solid;
}
.insurance_box .table-tr:last-child{
  border:0;
}
.insurance_box .table-tr .tab-title{
  font-size: 26rpx;
}
.insurance_box .table-tr .tab-content{
  border:0;
}
.coverBox .article_box{
  padding-top: 30rpx;
}
.coverBox .smTitle{
  padding-left: 32rpx;
  font-weight: bold;
}
.coverBox .smContent{
  padding-left: 32rpx;
  text-indent: 1em;
  font-size: 28rpx;
}
.confirmCenter{
  width:100%;
}
.weui-cell{
  height:88rpx;
  padding:0 15px;
}
.weui-cell>view{
  width: 100%;
  line-height: 88rpx;
  font-size: 30rpx;
  color:rgb(74,74,74);
}
.weui-cell .icon-gengduo{
  float: right;
  color:#c7c7cc;
}
.weui-cell:before{
  border-top-color:#c7c7cc;
  border-top-width: 1rpx;
}
